&apos;------------------------------------------------------------------------------------------------------------------------
&apos;несмотря на название этот метод и для альфа и для бета
sub insertBetaToTemplateStep(i, betaSourcesList as Collection, betaPointsList as Collection, betaValueList as Collection, sign as String, point_count as integer)
	dim bd as new DUnit 
	bd = _BDList.item(i)
	dim j as integer
	for j=0 to betaValueList.count-1
		m_point = betaValueList.item(j+1)
		
		if bd.isAlternativePoints then
			cont_point = bd.getAltList.item(j+1)
		else
			cont_point = betaPointsList.item(j+1)
		end if	
					
		dim srcName as string
		srcName = betaSourcesList.item(j+1)
		srcName = Replace (srcName,&quot;Bq&quot;,&quot;&quot;)
		srcName = Replace (srcName,&quot;/S&quot;,&quot;&quot;)
		findAndRename((sign + i + &quot;-3&quot; + j), srcName) rem название источника
	
		dim cont_point_formatted as double
		&apos;ВАЖНО. округляю значение контрольной точки альфа или бета, и только после этого вывожу значение в таблицу и вычисляю погрешность. 
		&apos;До этого округлялось только при выводе, расчет погрешности производился с полным несокращенным значением. В результате в таблице 
		&apos;могло быть при совпадении контрольной точки и измеренного значения погрешность не равная 0 (например было число 1.2345, а вывилось 1.2, а погрешность для этих двух чисел уже не будет равна 0)
		if cont_point&lt;500  then cont_point_formatted = Format(cont_point, &quot;0.0&quot;) rem активность источника
		if cont_point&gt;=500 then cont_point_formatted = Format(cont_point, &quot;0&quot;)   rem активность источника
		findAndRename((sign + i + &quot;-9&quot; + j), cont_point_formatted) rem активность источника
		findAndRename((sign + i + &quot;-9&quot; + j), cont_point_formatted) rem активность источника
				
		rem TODO сделать через select case
		if m_point &lt; 200 and m_point &gt; 0  then MyFormat = &quot;0.0&quot;
		if m_point &gt;= 200 				  then MyFormat = &quot;0&quot;

		if j=0 then
			fivePointArray = Math.avaragePointsRndFiveArray(m_point, percentOtk)
			findAndRename((sign + i + &quot;-i1&quot; + j), Format(fivePointArray(0), MyFormat)) rem измерение 1
			findAndRename((sign + i + &quot;-i2&quot; + j), Format(fivePointArray(1), MyFormat)) rem измерение 2
			findAndRename((sign + i + &quot;-i3&quot; + j), Format(fivePointArray(2), MyFormat)) rem измерение 3
			findAndRename((sign + i + &quot;-i4&quot; + j), Format(fivePointArray(3), MyFormat)) rem измерение 4
			findAndRename((sign + i + &quot;-i5&quot; + j), Format(fivePointArray(4), MyFormat)) rem измерение 5
		endif				
		
		threePointArray = Math.avaragePointsRndArray(m_point, percentOtk)		
		findAndRename((sign + i + &quot;-6&quot; + j), Format(threePointArray(0), MyFormat)) rem измерение 1
		findAndRename((sign + i + &quot;-7&quot; + j), Format(threePointArray(1), MyFormat)) rem измерение 2
		findAndRename((sign + i + &quot;-8&quot; + j), Format(threePointArray(2), MyFormat)) rem измерение 3
						
		if cont_point&lt;150 then 
			findAndRename((sign + i + &quot;-5&quot; + j), Activity.BETA_BG) rem фон
		else	
			findAndRename((sign + i + &quot;-5&quot; + j), &quot;---&quot;) 
		end if
				
		findAndRename((sign + i + &quot;-0&quot; + j), Format(m_point, MyFormat)) rem среднее по трем измерениям

		dim pogr_point as double
		pogr_point = Math.RelativeError(m_point, cont_point_formatted)
		MyFormat = &quot;0.00&quot;
		
		findAndRename((sign + i + &quot;-1&quot; + j), Format(pogr_point, MyFormat))
		findAndRename((sign + i + &quot;-2&quot; + j), Format(Math.ConfidenceLimit(Facility.getPercent(&quot;PERCENT_BETA&quot;), pogr_point), MyFormat))
	next j
end sub
