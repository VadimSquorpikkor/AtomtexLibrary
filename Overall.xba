&apos;------------------------------------------------------------------------------------------------------------------------
&apos;    регулярное выражение &quot;\$[^\$]+\$&quot; по частям:
&apos;    \$ - Экранированный символ доллара
&apos;    $ в regex обычно означает &quot;конец строки&quot;
&apos;    \$ означает буквально символ доллара
&apos;    Первое \$ ищет открывающий символ $
&apos;    [^\$]+ - Содержимое между долларами
&apos;    [ ] - обозначает набор символов
&apos;    ^ внутри скобок означает &quot;НЕ&quot; (отрицание)
&apos;    [^\$] = &quot;любой символ, КРОМЕ доллара&quot;
&apos;    + = &quot;один или более раз&quot;
&apos;    Вместе: [^\$]+ = &quot;один или более любых символов, кроме доллара&quot;
&apos;3. \$ - Закрывающий символ доллара
&apos;------------------------------------------------------------------------------------------------------------------------
&apos;находит все метки типа &quot;$supКАКОЙ-ТО_ТЕКСТ$&quot; и меняет на текст в суперскрипт формате (это нужно для нормального вывода текста в котором есть например хим элемент с индексом)
Sub findAndReplaceAllSuperscript
    Dim oDoc As Object
    Dim oSearch As Object
    Dim oFound As Object
    
    oDoc = ThisComponent
    oSearch = oDoc.createSearchDescriptor()
    oSearch.SearchString = &quot;\$sup[^\$]+\$&quot;
    oSearch.SearchRegularExpression = True
    
    oFound = oDoc.findFirst(oSearch)
    
    While Not IsNull(oFound)
        Dim originalText As String
        originalText = oFound.getString()
        
        &apos; Проверяем, что текст действительно между $
        If Left(originalText, 1) = &quot;$&quot; And Right(originalText, 1) = &quot;$&quot; Then
            Dim cleanText As String
            cleanText = Mid(originalText, 5, Len(originalText) - 5)
        	oFound.CharEscapement = 30 &apos;30% (стандартное значение)
        	oFound.CharEscapementHeight = 58 &apos;58% — стандартное уменьшение размера шрифта
            oFound.setString(cleanText)
        End If
        
        oFound = oDoc.findNext(oFound.getEnd(), oSearch)
    Wend
End Sub
&apos;------------------------------------------------------------------------------------------------------------------------
Sub findAndReplaceAllSubscript
    Dim oDoc As Object
    Dim oSearch As Object
    Dim oFound As Object
    
    oDoc = ThisComponent
    oSearch = oDoc.createSearchDescriptor()
    oSearch.SearchString = &quot;\$sub[^\$]+\$&quot;
    oSearch.SearchRegularExpression = True
    
    oFound = oDoc.findFirst(oSearch)
    
    While Not IsNull(oFound)
        Dim originalText As String
        originalText = oFound.getString()
        
        &apos; Проверяем, что текст действительно между $
        If Left(originalText, 1) = &quot;$&quot; And Right(originalText, 1) = &quot;$&quot; Then
            Dim cleanText As String
            cleanText = Mid(originalText, 5, Len(originalText) - 5)
        	oFound.CharEscapement = -25  &apos; -25% — стандартный subscript ВНИМАНИЕ! findAndReplaceAllSuperscript и findAndReplaceAllSubscript различаются только этой строчкой и регулярным выражением
        	oFound.CharEscapementHeight = 58 &apos;58% — стандартное уменьшение размера шрифта
            oFound.setString(cleanText)
        End If
        
        oFound = oDoc.findNext(oFound.getEnd(), oSearch)
    Wend
End Sub
